using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp11
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int quantityPictures = 52;
            int ranks = 3;
            int picturesInRanks = quantityPictures / ranks;
            int leftOutOfLine = quantityPictures % ranks;
            Console.WriteLine("Количество картинок в трех рядах = " + picturesInRanks);
            Console.WriteLine("Количество картинок оставшее вне ряда = " + leftOutOfLine);   
            Console.ReadKey();

        }
    }
}
