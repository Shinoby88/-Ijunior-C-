using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp11
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int allPictures = 52;
            int numberOfRows = 3;
            int picturesInRanks = allPictures / numberOfRows;
            int remainingPictures = allPictures % numberOfRows;
            Console.WriteLine("Количество картинок в трех рядах = " + picturesInRanks);
            Console.WriteLine("Количество картинок оставшиеся вне ряда = " + remainingPictures);   
            Console.ReadKey();

        }
    }
}
