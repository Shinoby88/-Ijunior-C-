using System;

namespace HealtBar
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int health = 5;
            int startBar = 0;
            int healthMax = 10;
            string symbolHealth = "#";
            string symbol = "_";
            string startSymbol = "[";
            string endSymbol = "]";
            string voidSymbol = "";

            DrawCircleBar(startBar, health, symbolHealth, startSymbol, voidSymbol);
            DrawCircleBar(health, healthMax, symbol, voidSymbol, endSymbol);
        }

        static void DrawCircleBar(int value, int valueMax, string symbolsInBar, string symbolsOne, string symbolsTwo)
        {
            string bar = "";

            for (int i = value; i < valueMax; i++)
            {
                bar = bar + symbolsInBar;
            }
            Console.Write(symbolsOne + bar + symbolsTwo);
        }


    }
}
