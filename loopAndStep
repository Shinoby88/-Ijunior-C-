using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp36
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int minDigital = 5;
            int maxDigital = 96;
            int step = 7;

            for (int i = minDigital;  i <= maxDigital; i += step)
            {
                Console.Write($" {i} ");
            }
                   
        }
    }
}
