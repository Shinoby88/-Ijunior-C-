using System;

namespace ConsoleApp17
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int[] digitals = { 1, 2, 3, 4, 5, 6 };        
            Shuffle(digitals);
            
            foreach (int item in digitals)
            {
                Console.Write(item + " ");
            }
        }
        public static void Shuffle(int[] array)
        {
            int lenghtArray = array.Length;
            Random random = new Random();

            for (int i = lenghtArray - 1; i > 0; i--)
            {                
                int offsetIndex = random.Next(0, lenghtArray);
                int temp = array[i];
                array[i] = array[offsetIndex];
                array[offsetIndex] = temp;
            }          
        }
    }
}
